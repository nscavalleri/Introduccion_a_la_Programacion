Algoritmo MesesDelAnio
	
	Dimension meses[12]
	Definir i Como Entero
	
	meses[1]= "Enero"
	meses[2]= "Febrero"
	meses[3]= "Marzo"
	meses[4]= "Abril"
	meses[5]= "Mayo"
	meses[6]= "Junio"
	meses[7]= "Julio"
	meses[8]= "Agosto"
	meses[9]= "Septiembre"
	meses[10]= "Octubre"
	meses[11]= "Noviembre"
	meses[12]= "Diciembre"
	
	Para i=1 Hasta 12 Hacer
		Escribir "El mes ",i," es ",meses[i]
	FinPara
	
	
FinAlgoritmo
